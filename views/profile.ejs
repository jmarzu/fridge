<h2>Profile page</h2>

<h3><a href="/">Home</a></h3>
<form action="/shoppinglist" method="GET">
  <button typy="submit" class="btn btn-primary">Shopping List</button>
</form>

<% favorite.forEach(function(favorites) { %>
  <div class="profileFavorites">
    <form action="/profile/delete/<%= favorites.userId %>" method="POST">
      <h4><a href="<%= favorites.href %>"><%= favorites.title %></a></h4>
      <input type="hidden" name="title" value="<%= favorites.title  %>">
      <input type="hidden" name="href" value="<%= favorites.href  %>">
      <input class="btn btn-primary red" type="submit" value="Delete">
    </form>
  </div>
<% }); %>